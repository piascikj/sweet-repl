{
  "exclude": [
    "^(node_modules|bower_components|\\.imdone|target|build)[\\/\\\\]?|\\.(git|svn)|\\~$|\\.(jpg|png|gif|swp|ttf|otf)$"
  ],
  "watcher": true,
  "keepEmptyPriority": true,
  "code": {
    "include_lists": [
      "TODO",
      "DOING",
      "DONE",
      "PLANNING",
      "FIXME",
      "ARCHIVE"
    ]
  },
  "lists": [
    {
      "name": "MIT",
      "hidden": false
    },
    {
      "name": "TODO",
      "hidden": false
    }
  ],
  "marked": {
    "gfm": true,
    "tables": true,
    "breaks": false,
    "pedantic": false,
    "sanitize": true,
    "smartLists": true,
    "langPrefix": "language-"
  },
  "meta": {
    "gh": {
      "urlTemplate": "https://github.com/piascikj/sweet-repl/issues/%s",
      "titleTemplate": "View github issue %s",
      "icon": "icon-octoface"
    }
  }
}